@{
  Layout = "_Layout";
}
@using Factory.Models;


<h1>Master Factory List: </h1>

<p class="titles">Currently Employed Engineers: </p>
@foreach (Engineer engineer in @Model["engineers"])
{
  <li class="name">@($"{engineer.EngineerName}")</li> 
}
<br>
<p class="titles">All Machines: </p>

@foreach (Machine machine in @Model["machines"])
{
  <li class="name">@($"{machine.MachineName}")</li>
}
<br>

